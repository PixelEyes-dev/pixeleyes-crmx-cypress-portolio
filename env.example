# CRMx Cypress Test Environment Variables
# Copy this file to .env and fill in your actual values

# Application Configuration
CYPRESS_BASE_URL=https://your-test-environment.com
CYPRESS_ENVIRONMENT=test

# Test User Credentials (use test accounts only)
CYPRESS_USER_EMAIL=test@example.com
CYPRESS_USER_PASSWORD=your_test_password

# Supabase Configuration
CYPRESS_SUPABASE_URL=https://your-project.supabase.co
CYPRESS_SUPABASE_ANON_KEY=your_supabase_anon_key

# Database Connection (for test data cleanup)
SUPABASE_DB_HOST=your-db-host.supabase.co
SUPABASE_DB_PORT=5432
SUPABASE_DB_NAME=postgres
SUPABASE_DB_USER=your_db_user
SUPABASE_DB_PASSWORD=your_db_password
SUPABASE_DB_SSL=true

# Optional: SSL Certificate Path
# SUPABASE_DB_SSL_CERT_PATH=./certs/your-cert.crt 