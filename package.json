{
  "name": "pixeleyes-crmx-cypress-portolio",
  "version": "1.0.0",
  "description": "CRMx cypress tests!!!",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headed": "cypress run --headed",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:firefox": "cypress run --browser firefox",
    "cypress:run:edge": "cypress run --browser edge",
    "cypress:cross-browser": "node scripts/runCrossBrowserTests.js",
    "cypress:cross-browser:chrome": "cypress run --browser chrome --spec cypress/e2e/leads/crossBrowserLeadManagement.cy.js",
    "cypress:cross-browser:firefox": "cypress run --browser firefox --spec cypress/e2e/leads/crossBrowserLeadManagement.cy.js",
    "cypress:cross-browser:edge": "cypress run --browser edge --spec cypress/e2e/leads/crossBrowserLeadManagement.cy.js",
    "cypress:cross-browser:safari": "cypress run --browser safari --spec cypress/e2e/leads/crossBrowserLeadManagement.cy.js",
    "cypress:generate-html-report": "node scripts/generateHTMLReport.js",
    "visual:generate-baselines": "cypress run --spec cypress/e2e/visual-regression/criticalUIStates.cy.js --env baseline=true",
    "visual:generate-browser-baselines": "node scripts/generateBrowserBaselines.js",
    "visual:run-tests": "cypress run --spec cypress/e2e/visual-regression/criticalUIStates.cy.js",
    "visual:run-cross-browser": "node scripts/runCrossBrowserVisualRegression.js",
    "visual:open": "cypress open --spec cypress/e2e/visual-regression/criticalUIStates.cy.js",
    "visual:compare": "cypress run --spec cypress/e2e/visual-regression/criticalUIStates.cy.js --env compare=true",
    "visual:compare-local": "node scripts/compareVisualRegression.js",
    "visual:compare-chrome": "node scripts/compareVisualRegression.js -- --browser=chrome",
    "visual:compare-firefox": "node scripts/compareVisualRegression.js -- --browser=firefox",
    "visual:compare-edge": "node scripts/compareVisualRegression.js -- --browser=edge",
    "visual:compare-all-browsers": "node scripts/compareVisualRegression.js -- --all-browsers",
    "visual:compare-main": "node scripts/compareVisualRegression.js -- --main",
    "visual:open-viewer": "start cypress/screenshots/visual-regression/comparison-viewer.html",
    "visual:open-side-by-side": "start cypress/screenshots/visual-regression/side-by-side-comparison.html",
    "visual:open-directories": "explorer cypress/screenshots/visual-regression/baselines && explorer cypress/screenshots/cross-browser/criticalUIStates.cy.js",
    "test": "cypress run",
    "test:open": "cypress open",
    "test:report": "cypress run",
    "clean-reports": "rimraf cypress/reports && mkdir cypress/reports"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gp-pixeleyes/pixeleyes-crmx-cypress-portolio.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gp-pixeleyes/pixeleyes-crmx-cypress-portolio/issues"
  },
  "homepage": "https://github.com/gp-pixeleyes/pixeleyes-crmx-cypress-portolio#readme",
  "engines": {
    "node": ">=18.19.0"
  },
  "devDependencies": {
    "@cypress/browserify-preprocessor": "latest",
    "@supabase/supabase-js": "^2.39.0",
    "@types/pg": "^8.15.4",
    "cypress": "^15.0.0",
    "cypress-mochawesome-reporter": "^4.0.2",
    "cypress-visual-regression": "^5.3.0",
    "dotenv": "^16.5.0",
    "pg": "^8.16.2",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^23.0.0"
  }
}
